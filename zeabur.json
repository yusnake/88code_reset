{
  "name": "88code-reset",
  "description": "88code.org 订阅自动重置工具 - 自动管理你的积分额度",
  "env": {
    "API_KEY": {
      "description": "你的 88code.org API Key（必填）",
      "required": true
    },
    "TARGET_PLANS": {
      "description": "目标套餐名称（匹配 subscriptionName，可选；留空表示所有 MONTHLY 套餐）",
      "default": "",
      "required": false
    },
    "TZ": {
      "description": "时区设置，例如：Asia/Shanghai、Asia/Tokyo",
      "default": "Asia/Shanghai",
      "required": false
    }
  }
}
